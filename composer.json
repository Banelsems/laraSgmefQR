{
  "name": "banelsems/lara-sgmef-qr",
  "description": "Package for integrating with the SGMEF API Benin and generating QR codes in Laravel.",
  "keywords": [
    "laravel",
    "qrcode",
    "benin",
    "sgmef",
    "invoiceNormalize"
  ],
  "homepage": "https://github.com/banelsems/lara-sgmef-qr",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Banelsems\\LaraSgmefQr\\": "src/"
    }
  },
  "authors": [
    {
      "name": "SEMASSOUSSI Banel Fajardy",
      "email": "banelsemassoussi@gmail.com",
      "homepage": "https://banelsems.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/http": "^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload-dev": {
    "psr-4": {
      "Banelsems\\LaraSgmefQr\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Banelsems\\LaraSgmefQr\\Provider"
      ],
      "aliases": {
        "LaraSgmefQr": "Banelsems\\LaraSgmefQr\\Facade"
      }
    }
  },
  "publish": [
    "config/lara-sgmef-qr.php",
    "resources/views/invoiceNormalize.blade.php"
  ],
  "support": {
    "forum": "https://lara-sgmef-qr.discussion.group",
    "issues": "https://github.com/banelsems/lara-sgmef-qr/issues"
  }
}
