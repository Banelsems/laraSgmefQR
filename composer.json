{
  "name": "banelsems/lara-sgmef-qr",
  "description": "Modern Laravel package for integrating with the SGMEF API (Benin electronic invoicing) with Clean Code architecture.",
  "type": "library",
  "keywords": [
    "laravel",
    "benin",
    "sgmef",
    "electronic-invoicing",
    "qrcode",
    "clean-code",
    "solid"
  ],
  "homepage": "https://github.com/banelsems/lara-sgmef-qr",
  "license": "MIT",
  "authors": [
    {
      "name": "SEMASSOUSSI Banel Fajardy",
      "email": "banelsemassoussi@gmail.com",
      "homepage": "https://banelsems.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "laravel/framework": "^10.0",
    "guzzlehttp/guzzle": "^7.0",
    "ext-json": "*",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "orchestra/testbench": "^8.0",
    "mockery/mockery": "^1.5",
    "phpstan/phpstan": "^1.10",
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Banelsems\\LaraSgmefQr\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Banelsems\\LaraSgmefQr\\Tests\\": "src/Tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Banelsems\\LaraSgmefQr\\Providers\\LaraSgmefQRServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-html coverage",
    "analyse": "phpstan analyse",
    "cs-check": "php-cs-fixer fix --dry-run --diff",
    "cs-fix": "php-cs-fixer fix"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/banelsems/lara-sgmef-qr/issues",
    "source": "https://github.com/banelsems/lara-sgmef-qr",
    "docs": "https://github.com/banelsems/lara-sgmef-qr/wiki"
  }
}
